// envmaptint_fix
"LightmappedGeneric"
{
	"$basetexture" "concrete/concretefloor037b"
	"$surfaceprop" "concrete"
	"%keywords" "c17downtown"
	"$bumpmap" "Nature/slime_normal"
//	"$bumpmap" "dev/water_normal"

	"LightmappedGeneric_DX9"
	{
		"$normalmapalphaenvmapmask" 1
		"$envmap" "env_cubemap"
		"$envmapsaturation" 1
		"$envmapcontrast" 1
		"$envmaptint" "[.4 .4 .4]"
	}
		"Proxies"
	{
		"AnimatedTexture"
		{
			"animatedtexturevar" "$bumpmap"
			"animatedtextureframenumvar" "$bumpframe"
			"animatedtextureframerate" 20.00
		}
	}
}
